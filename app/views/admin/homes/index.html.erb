<div id="page-inner">
  <div class="row">
    <div class="col-lg-12">
    <div class="panel panel-primary">
      <div class="panel-heading"><i class="fa fa-list"></i><%= t(".in_title") %></div>
      <div class="panel-body">
        <table class="table table-bordered">
          <thead>
            <tr>
              <th><%= t(".in_a") %></th>
              <th><%= t(".in_b") %></th>
              <th><%= t(".in_c") %></th>
              <th><%= t(".in_d") %></th>
              <th><%= t(".in_e") %></th>
              <th><%= t(".in_f") %></th>
            </tr>
          </thead>
          <tbody>
            <% if(!@homes.nil?) %>
                <%= paginate @homes, theme: "twitter-bootstrap-3" %>
                <%= render partial: "home",  collection: @homes, as: "home" %>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
    <button class="btn btn-success"><i class="fa fa-plus"> <%= link_to t(".in_g") , new_admin_home_path %></i></button>
    </div>
  </div>
  <hr />
</div>
